{
    "name": "taro-snippets",
    "displayName": "taro-snippets",
    "description": "生成taro组件代码以及使用taro方法提示",
    "version": "0.0.5",
    "publisher": "hacker",
    "icon": "assets/icon.png",
    "engines": {
      "vscode": "^1.30.0"
    },
    "categories": [
      "Snippets"
    ],
    "contributes": {
      "languages": [
        {
          "id": "typescriptreact",
          "extensions": [
            ".tsx",
            ".jsx"
          ]
        }
      ],
      "snippets": [
        {
          "language": "typescriptreact",
          "path": "./snippets/snippets.code-snippets"
        }
      ]
    }
}
